<div class="login_box">
<%= form_tag login_path do %>

<h3><%= label_tag :username %></h3>
<%= text_field_tag :username %>
<h3><%= label_tag :password %></h3>
<%= password_field_tag :password %><br>
<h3><%= submit_tag "login" %></h3>

<% end %>
</div>

<h1>login with google:</h1>
<h1><%= link_to "Google", "/auth/google_oauth2" %></h1>
